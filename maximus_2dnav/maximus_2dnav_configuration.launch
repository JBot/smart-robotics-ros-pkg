<launch>
  <node pkg="neato_xv11_lds" type="neato_xv11_lds_publisher" name="neato_xv11_lds_publisher" output="screen">
  </node>

  <node pkg="avr_bridge" type="bridge_node.py" name="avr_odom_bridge_node" output="screen">
    <rosparam file="$(find maximus_position)/motionControlROS.yaml" command="load" />
  </node>

  <node pkg="maximus_position" type="maximus_transform_pose" name="maximus_pose_node" output="screen">
  </node>

  <node pkg="maximus_setup_tf" type="tf_broadcaster" name="maximus_tf_broadcaster" output="screen">
  </node>

</launch>
