<launch>
  <node pkg="neato_xv11_lds" type="neato_xv11_lds_publisher_natif" name="lds_publisher" output="screen">
  </node>

  <node pkg="maximus_position" type="maximus_transform_pose" name="maximus_pose_node" output="screen">
  </node>

  <node pkg="maximus_setup_tf" type="tf_broadcaster" name="maximus_tf_broadcaster" output="screen">
  </node>

  <node pkg="eurobot_obstacles" type="pc_eurobot_obstacles" name="eurobot_obstacles" output="screen">
  </node>

  <node pkg="map_server" type="map_server" name="eurobot_map" args="$(find maximus_nav)/mymap.yaml" output="screen">
  </node>

  <node pkg="maximus_position" type="maximus_pathwrapper" name="maximus_pathwrapper" output="screen">
  </node>

  <node pkg="rosserial_python" type="serial_node.py" name="arduino_serial_odometry" output="screen">
    <param name="baud" value="115200" />
    <param name="port" value="/dev/ttyUSB1" />
  </node>
</launch>
